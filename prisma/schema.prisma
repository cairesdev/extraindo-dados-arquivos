// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model DadosExatraidosFolhaDePagementos{
  id              String @id @default(uuid())
  nome			 			String
  vinculo         String
  mes_periodo     String
  ano	            String
  orgao           String
  cpf	            String
  matricula	      String
  cargo           String
  dataAdmissao	  DateTime  @default(now())
  cargaHoraria	  String
  valorBruto	    Float
  valorLiquido	  Float
  valorDesconto	  Float		
}